{
    "name": "@xenobytezero/strapi-plugin-react-editorjs",
    "description": "Plugin for Strapi Headless CMS, hiding the standard WYSIWYG editor and replacing it with Editor.js",
    "version": "0.8.6",
    "author": {
        "name": "Matvey Melishev",
        "email": "matvey@melishev.ru",
        "url": "https://melishev.ru"
    },
    "bugs": {
        "url": "https://github.com/xenobytezero/strapi-plugin-react-editorjs/issues"
    },
    "config": {
        "commitizen": {
            "path": "./node_modules/cz-conventional-changelog"
        }
    },
    "dependencies": {
        "@editorjs/editorjs": "2.30.7",
        "@editorjs/image": "2.10.1",
        "@editorjs/paragraph": "2.11.7",
        "@strapi/design-system": "2.0.0-rc.14",
        "@strapi/icons": "2.0.0-rc.14",
        "@types/typography": "^0.16.8",
        "axios": "^1.4.0",
        "classnames": "^2.3.1",
        "get-file-object-from-local-path": "1.0.2",
        "open-graph-scraper": "4.9.2",
        "react-intl": "^7.0.1",
        "typography": "^0.16.24"
    },
    "devDependencies": {
        "@semantic-release/changelog": "^6.0.1",
        "@semantic-release/git": "^10.0.1",
        "@strapi/design-system": "2.0.0-rc.14",
        "@strapi/sdk-plugin": "5.2.7",
        "@strapi/strapi": "5.6.0",
        "@strapi/upload": "5.6.0",
        "@strapi/utils": "5.6.0",
        "@types/koa": "^2.13.5",
        "@types/react": "^18.3.13",
        "cz-conventional-changelog": "^3.3.0",
        "prop-types": "^15.8.1",
        "react": "^18.3.1",
        "react-dom": "^18.3.1",
        "react-editor-js": "2.1.0",
        "react-router-dom": "^7.0.2",
        "semantic-release": "^19.0.2",
        "styled-components": "^6.1.13",
        "typescript": "5.7.2"
    },
    "engines": {
        "node": ">=10.16.0 <=18.x.x",
        "npm": ">=6.0.0"
    },
    "exports": {
        "./package.json": "./package.json",
        "./strapi-admin": {
            "types": "./dist/admin/src/index.d.ts",
            "source": "./admin/src/index.ts",
            "import": "./dist/admin/index.mjs",
            "require": "./dist/admin/index.js",
            "default": "./dist/admin/index.js"
        },
        "./strapi-server": {
            "types": "./dist/server/src/index.d.ts",
            "source": "./server/src/index.ts",
            "import": "./dist/server/index.mjs",
            "require": "./dist/server/index.js",
            "default": "./dist/server/index.js"
        },
        "./types/*": "./dist-types/*.d.ts"
    },
    "files": [
        "dist"
    ],
    "homepage": "https://market.strapi.io/plugins/strapi-plugin-react-editorjs",
    "keywords": [
        "editor-js",
        "plugin",
        "strapi",
        "wysiwyg"
    ],
    "license": "MIT",
    "maintainers": [
        {
            "name": "Matvey Melishev",
            "email": "matvey@melishev.ru",
            "url": "https://melishev.ru"
        },
        {
            "name": "Jason Skipper",
            "url": "https://www.skipperinnovations.com"
        }
    ],
    "peerDependencies": {
        "@strapi/sdk-plugin": "5.2.7",
        "@strapi/strapi": "5.6.0",
        "react": "^18.3.1",
        "react-dom": "^18.3.1",
        "react-router-dom": "^6.28.0",
        "styled-components": "^6.1.13"
    },
    "publishConfig": {
        "registry": "https://npm.pkg.github.com"
    },
    "repository": {
        "type": "git",
        "url": "https://github.com/xenobytezero/strapi-plugin-react-editorjs"
    },
    "scripts": {
        "build": "strapi-plugin build --sourcemap",
        "verify": "strapi-plugin verify",
        "watch": "strapi-plugin watch",
        "types": "tsc --project ./types"
    },
    "strapi": {
        "icon": "plug",
        "name": "editorjs",
        "displayName": "EditorJS",
        "description": "Plugin for Strapi Headless CMS, hiding the standard WYSIWYG editor and replacing it with Editor.js",
        "required": true,
        "kind": "plugin"
    }
}